{
  "name": "Crouton-clip",
  "description": "Crouton clipboard synchronization.",
  "version": "0.1",
  "incognito": "split",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    { "socket" : [ "tcp-listen:127.0.0.1:30001"] }
  ]
}
